{
    "version": "0.2.0",
    "configurations": [
        
      {
        "type": "java",
        "request": "launch",
        "name": "BackendApplication",
        "mainClass": "com.example.backend.BackendApplication",
        "projectName": "backend"
      },
      {
        "type": "debugpy",
        "request": "launch",
        "name": "FlaskApp",
        "program": "${workspaceFolder}/ai_Model/EndPoints.py",
        "console": "integratedTerminal",
        "env": {
          "FLASK_APP": "ai_Model/EndPoints.py",
          "FLASK_ENV": "development"
        },
        "justMyCode": true
      },
      {
      "type": "node",
      "request": "launch",
      "name": "Angular Frontend",
      "runtimeExecutable": "npm",
      "cwd": "${workspaceFolder}/../frontend",
       "runtimeArgs": [
        
    
    "start"
  ],
      "console": "integratedTerminal"
    }
    ],
    "compounds": [
      {
        "name": "Start Java + Flask + Angular Frontend",
        "configurations": [
          "BackendApplication",
          "FlaskApp",
          "Angular Frontend"
        ]
      }
    ]
  }
  